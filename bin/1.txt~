BEGIN METHOD One
	ID 1

	BLOCK DEPENDENCIES
	0 RECORDS
	ENDBLOCK DEPENDENCIES

	BLOCK PARAMETERS
	1 RECORD
		molecule = LIST methane ethane 
	ENDBLOCK PARAMETERS

	BLOCK SCRIPTS
	1 RECORD

		SCRIPT run3DRISM

			echo $A > one.txt

		ENDSCRIPT calculateFreeEnergy

	ENDBLOCK SCRIPTS

END METHOD 


BEGIN METHOD Two
	ID 2

	BLOCK DEPENDENCIES
	0 RECORDS
	ENDBLOCK DEPENDENCIES

	BLOCK PARAMETERS
	2 RECORDS
		b = LIST hello world (default)
		molecule = LIST methane ethane 
	ENDBLOCK PARAMETERS

	BLOCK SCRIPTS
	1 RECORD
	
		SCRIPT two
	
			echo $B > two.txt

		ENDSCRIPT


	ENDBLOCK SCRIPTS

END METHOD 

BEGIN METHOD Three
	ID 3
	
	BLOCK DEPENDENCIES
	2 RECORDS
		METHOD 1 AS one 
		METHOD 2 AS two 
	ENDBLOCK DEPENDENCIES

	BLOCK PARAMETERS
	2 RECORDS	
		c = LIST 11.2 (default) 15.8
		molecule = LIST methane ethane 
	ENDBLOCK PARAMETERS

	BLOCK SCRIPTS
	1 RECORD

		SCRIPT three

			cat $one/one.txt $two/two.txt > three.txt
			echo $C >> three.txt

		ENDSCRIPT

	ENDBLOCK SCRIPTS

END METHOD


